  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM", "es6" , "es2017"],
    "allowSyntheticDefaultImports": true
  },
  "include": ["vite.config.ts", "build/**/*.ts", "build/**/*.json", "*.json"]
}
